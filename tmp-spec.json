  "components": {
    "schemas": {
      "ReviewRequest": {
        "type": "object",
        "properties": {
          "productOptionId": { "type": "integer", "format": "int64" },
          "rating": { "type": "integer", "format": "int32" },
          "positiveComment": {
            "type": "string",
            "maxLength": 1500,
            "minLength": 15
          },
          "negativeComment": {
            "type": "string",
            "maxLength": 1500,
            "minLength": 15
          },
          "mediaUrl": { "type": "array", "items": { "type": "string" } },
          "receiptUrl": { "type": "array", "items": { "type": "string" } }
        },
        "required": [
          "negativeComment",
          "positiveComment",
          "productOptionId",
          "rating"
        ]
      },
      "ApiResponseReviewResponse": {
        "type": "object",
        "properties": {
          "success": { "type": "boolean" },
          "status": { "type": "integer", "format": "int32" },
          "message": { "type": "string" },
          "data": { "$ref": "#/components/schemas/ReviewResponse" }
        }
      },
      "ReviewResponse": {
        "type": "object",
        "properties": { "reviewId": { "type": "integer", "format": "int64" } }
      },
      "ReviewAdminRequest": {
        "type": "object",
        "properties": {
          "productOptionId": { "type": "integer", "format": "int64" },
          "rating": { "type": "integer", "format": "int32" },
          "positiveComment": {
            "type": "string",
            "maxLength": 1500,
            "minLength": 15
          },
          "negativeComment": {
            "type": "string",
            "maxLength": 1500,
            "minLength": 15
          },
          "mediaType": { "type": "string", "enum": ["IMAGE", "VIDEO"] },
          "videoUrl": { "type": "string" },
          "imageUrl": {
            "type": "array",
            "items": { "type": "string" },
            "maxItems": 2147483647,
            "minItems": 1
          },
          "receiptUrl": { "type": "string" }
        },
        "required": [
          "mediaType",
          "negativeComment",
          "positiveComment",
          "productOptionId",
          "rating"
        ]
      },
      "ApiResponseVoid": {
        "type": "object",
        "properties": {
          "success": { "type": "boolean" },
          "status": { "type": "integer", "format": "int32" },
          "message": { "type": "string" },
          "data": {}
        }
      },
      "ReviewReceiptRequest": {
        "type": "object",
        "properties": { "mediaType": { "type": "string" } },
        "required": ["mediaType"]
      },
      "ApiResponseReviewReceiptResponse": {
        "type": "object",
        "properties": {
          "success": { "type": "boolean" },
